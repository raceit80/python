    "import math\n",
    "\n",
    "def newton_sqrt_estimate(a, epsilon):\n",
    "    i = 1.000000000000\n",
    "    while abs(a  - (i*i)) > epsilon:\n",
    "        i = 0.5 * (i + a/i)\n",
    "        print (i)\n",
    "    \n"
   ]
